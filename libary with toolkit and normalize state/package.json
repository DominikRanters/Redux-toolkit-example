        "date-fns": "^2.16.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "redux-logger": "^3.0.6"
    },
    "devDependencies": {
        "chayns-toolkit": "^1.5.5",
        "prettier": "^2.1.2",
        "prettier-plugin-packagejson": "^2.2.9"
